{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 200,
    "tabWidth": 4,
    "endOfLine": "lf",
    "plugins": [
        "@ianvs/prettier-plugin-sort-imports"
    ],
    "importOrder": [
        "<TYPES>^(node:)",
        "<TYPES>",
        "<TYPES>^[.]",
        "",
        "<BUILTIN_MODULES>",
        "",
        "<THIRD_PARTY_MODULES>",
        "",
        "^@sfc/.+",
        "",
        "^[./]"
    ],
    "importOrderParserPlugins": [
        "typescript",
        "jsx",
        "decorators-legacy"
    ],
    "importOrderTypeScriptVersion": "5.0.0",
    "importOrderCaseSensitive": false,
    "overrides": [
        {
            "files": [
                "*.yml",
                "*.yaml"
            ],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}